{
    "name": "gzzl/cinter",
    "description": "GzzL Cinter",
    "keywords": [],
    "homepage": "https://github.com/gzzlee/cinter",
    "license": "MIT",
    "authors": [
        {
            "name": "GzzL",
            "email": "gzzl@example.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0|^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "orchestra/testbench": "^8.8",
        "pestphp/pest": "^2.20",
        "pestphp/pest-plugin-arch": "^2.5",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Gzzl\\GzzlCinter\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gzzl\\GzzlCinter\\GzzlCinterServiceProvider"
            ],
            "aliases": {
                "GzzlCinter": "Gzzl\\GzzlCinter\\Facades\\GzzlCinter"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
